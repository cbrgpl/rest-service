{
    "compilerOptions": {
      "target": "es2016",
      "allowSyntheticDefaultImports": false,
      "baseUrl": "./",
      "paths": {
        "@/*": [
          "libs/*"
        ],
        "@error/*": [
          "libs/error/*"
        ],
        "@helper/*": [
          "libs/helper/*"
        ],
        "@service/*": [
          "libs/service/*"
        ],
        "#CONST": [
          "libs/helpers/consts.js"
        ]
      }
    },
    "exclude": [
      "node_modules",
      "dist"
    ]
  }
